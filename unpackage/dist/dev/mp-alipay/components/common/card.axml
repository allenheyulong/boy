<view class="card"><block a:if="{{showhead}}"><view class="{{(('p-2 main-border-color')+' '+getHeadClass)}}"><block a:if="{{$slots.title}}"><slot name="title"></slot></block><block a:else><block a:if="{{headTitle}}"><text class="{{(('font-md')+' '+(headTitleWeight?'font-weight':''))}}">{{headTitle}}</text></block></block></view></block><view class="{{(getBodyClass)}}" style="{{(bodyStyle)}}"><block a:if="{{bodyCover}}"><image src="{{bodyCover}}" mode="widthFix"></image></block><slot></slot></view></view>